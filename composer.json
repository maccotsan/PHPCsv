{
    "name": "maccotsan/csv",
    "description": "",
    "keywords": ["csv"],
    "type": "library",
    "homepage": "https://github.com/maccotsan/PHPCsv/",
    "license": "MIT",
    "authors": [
        {
            "name": "maccotsan",
            "email": "maccotsan@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/maccotsan/PHPStringTool.git"
        }
    ],
    "require": {
        "php": ">=5.6",
        "maccotsan/stringtool": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "5.5"
    },
    "autoload": {
        "psr-4": {
            "maccotsan\\Csv\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "maccotsan\\Csv\\": "tests/"
        }
    },
    "scripts": {
